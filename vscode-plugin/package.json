{
	"name": "programmatic-templates",
	"displayName": "progTemplates",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"publisher": "sktguha",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "progTemplates",
			"properties": {
				"progTemplates.ProgrammaticTemplatePath": {
					"type": "string",
					"default": null,
					"description": "The absolute path to the programmatic template script. Only one script is supported at a time. If you want to use multiple, you can call them from your script and combine them in whatever way you want. TODO: add such an example"
				}
			}
		},
		"commands": [
			{
				"command": "progTemplates.execAndReplaceSelection",
				"title": "Execute the script and relace selected text with the output"
			}
		],
		"keybindings": [
			{
				"command": "progTemplates.execAndReplaceSelection",
				"key": "ctrl+z",
				"mac": "ctrl+z"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"package": "vsce package",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"axios": "^0.19.2",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"import-fresh": "^3.2.1"
	}
}
